function enroll_crs(tid,pid,uid)
{
	
	document.getElementById("watch_this_cr").innerHTML = "";

	
	var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
  
	  
	  if( this.responseText == 1 )
	  {
		 document.getElementById("watch_this_cr").innerHTML = "You enrolled in this course"; 

window.location.reload();


	  }
	  else
	  {

document.getElementById("watch_this_cr").disabled = false;		  
		  
		  
	  }
	  
	  
	  
    }
  };
  
  
  
  xhttp.open("GET", "/wp-content/themes/wplms-child/ajaxrequest_enrol.php?tid="+tid+"&cid="+pid+"&sid="+uid, true);
  xhttp.send();
	
	
	
	
	
	
}


















function ajax_save_user_page_for_redirect()
{
	

var link = window.location.href;
	
	var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
	  
	  
	  
    }
  };
  
  
  
  xhttp.open("GET", "/wp-content/themes/wplms-child/ajax_redirect_user.php?currurl="+link, true);
  xhttp.send();
	
	
	
	
	
	
}



