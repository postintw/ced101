<Script>

function showlangmenu()
{
document.getElementById('current_lng').style.color = '#AD2625';
document.getElementById('second_lang').style.display = 'inline-block';
document.getElementById('third_lang').style.display = 'inline-block';
}


function hidelangmenu()
{
document.getElementById('current_lng').style.color = '#bdbdbd';
document.getElementById('second_lang').style.display = 'none';
document.getElementById('third_lang').style.display = 'none';
}





var cnt = 0;
function show_menu(status)
{
cnt++;

if( cnt == 1 )
{

document.getElementById('slidemenu').style.display = 'block';
document.getElementById('slidemenu').style.opacity = '1';
}


if( cnt == 2 )
{

document.getElementById('slidemenu').style.display = 'none';
document.getElementById('slidemenu').style.opacity = '0';
cnt = 0;
}




}



</script>

 			
						
						
