define("app/event/headerNav",function(require,exports){"use strict";exports.init=function(){var $gHeaderRwd=$("#g-header-rwd");var $mobileNavIndicatorWrap=$gHeaderRwd.find(".mobile-nav-indicator-wrap");var backdrop=require("ui/backdrop");var hideMobileNav=function(){$gHeaderRwd.removeClass("full");backdrop.hide()};var showMobileNav=function(){$gHeaderRwd.addClass("full");backdrop.show();backdrop.onclick(hideMobileNav)};$mobileNavIndicatorWrap.on("click",function(e){if($gHeaderRwd.hasClass("full")){hideMobileNav()}else{showMobileNav()}})}});define("app/event/init",function(require,exports){"use strict";var request=require("app/modules/request");var init=function(){request({url:"/apiv2/cart/num",type:"GET"}).done(function(resp){if(resp.result&&resp.result[0].cart_num){var number=parseInt(resp.result[0].cart_num);if(number>99){number="99+"}$('<span class="num">'+resp.result[0].cart_num+"</span>").insertBefore($("#g-header-rwd .cart-wrapper a"))}})};return init});